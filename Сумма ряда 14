
#include <iostream>
#include <math.h>
#include <cmath>

using namespace std;

int main() 
{
	int n_max = 10;
	double sum = 0.0;
	for (int i = 1; i < n_max + 1; i++)
	{
		sum = (2 * i + 1 / i * i * (pow(i + 1, 2)));
	}
	cout << "Summa: " << sum;
	return 0;
}

import math

n_max = 10
sum = 0
q = 1
for n in range(1, n_max + 1):
    q = (2 * n + 1 / n**2 * (n + 1)**2)
    sum += q
print('Сумма ряда ', sum)
